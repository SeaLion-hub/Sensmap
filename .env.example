# 데이터베이스 설정
DB_HOST=localhost
DB_PORT=5432
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password

# 서버 설정
PORT=3000
NODE_ENV=development

# 세션 보안 (로그인 기능 추가 시 필요)
SESSION_SECRET=your_super_secret_key_here

# Railway 배포 시 참고사항:
